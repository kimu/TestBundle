            "homepage":  "http://www.infinity-tracking.com"
        }
    ],
    "require": {
        "php": ">=5.6.0",
        "symfony/symfony": "~3.3",
        "phpspec/phpspec": "2.5.*@dev",
        "phpunit/phpunit": "~5.7.0"
